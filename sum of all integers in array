#include<iostream>
using namespace std;

int arraysum(int arr[],int n)
{
    int sum=0;
    for (int i=0;i<n;i++)
    {
        sum=sum+arr[i];
    }

    return sum;
}

int main()
{
    int arr[10],n;
    cout<<"Enter the number of integers to be inserted :  ";
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }

    cout<<"Sum of all integers in array : "<<arraysum(arr,n);
}
